a + b, 0);\n\n  const expenses = transactionsAmount\n    .filter((amount) => amount < 0)\n    .reduce((a, b) => a + b, 0);\n\n  const incomes = transactionsAmount\n    .filter((amount) => amount > 0)\n    .reduce((a, b) => a + b, 0);\n\n  return (\n    <Container>\n      <Row>\n        <Col>\n          <h5>Total</h5>\n          <h2>{total.toFixed(2)} PLN</h2>\n        </Col>\n        <Col>\n          <h5>Expenses</h5>\n          <h2>{Math.abs(expenses).toFixed(2)} PLN</h2>\n        </Col>\n        <Col>\n          <h5>Incomes</h5>\n          <h2>{incomes.toFixed(2)} PLN</h2>\n        </Col>\n      </Row>\n      <Row>\n        <Col></Col>\n        <Col></Col>\n      </Row>\n      <Row>\n        <Col>\n          <div>\n            <hr />\n            <div>\n              <div>\n                <h5>Last transactions</h5>\n                <div>\n                  <Button className=\"button-sort\" variant=\"sort\">\n                    This month\n                  </Button>\n                  <Button className=\"button-sort\" variant=\"sort\">\n                    Expense\n                  </Button>\n                  <Button className=\"button-sort\" variant=\"sort\">\n                    Income\n                  </Button>\n                  <Button className=\"button-sort\" variant=\"sort\">\n                    Asc\n                  </Button>\n                  <Button className=\"button-sort\" variant=\"sort\">\n                    Desc\n                  </Button>\n                </div>\n                <div className=\"table\">\n                  {transactionsObj.map((transaction, key) => {\n                    return (\n                      <div key={key}>\n                        <TransactionRow\n                          key={transaction.id}\n                          text={transaction.text}\n                          amount={transaction.amount}\n                          option={transaction.option}\n                          date={transaction.date}\n     